{
  "name": "cashay-schema-loader",
  "version": "0.1.0",
  "description": "webpack loader that keeps your client schema fresh",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel -d lib/ src/",
    "watch": "babel -w -d lib/ src/",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattkrick/cashay-schema-loader.git"
  },
  "keywords": [
    "cashay",
    "graphql",
    "webpack",
    "loader"
  ],
  "author": "Simen Brekken",
  "contributors": [
    "Matt Krick <matt.krick@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattkrick/cashay-schema-loader/issues"
  },
  "homepage": "https://github.com/mattkrick/cashay-schema-loader#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "graphql": "0.6.0"
  },
  "peerDependencies": {
    "cashay": "^0.5.10",
    "webpack": "1 || ^2.1.0-beta"
  }
}
